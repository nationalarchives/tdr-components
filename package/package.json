    },
    "./nationalarchives/": "./nationalarchives/"
  },
  "sass": "nationalarchives/all.scss",
  "engines": {
    "node": ">= 4.2.0"
  },
  "author": {
    "name": "Transfer Digital Records",
    "email": "tdr@nationalarchives.gov.uk"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nationalarchives/tdr-components.git"
  },
  "license": "MIT",
  "dependencies": {
    "govuk-frontend": "^4.3.1"
  }
}
