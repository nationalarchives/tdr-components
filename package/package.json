{
  "name": "@nationalarchives/tdr-components",
  "description": "National Archives components based on the GovUK frontend",
  "version": "1.0.0",
  "main": "nationalarchives/all.js",
  "assets": ["govuk-frontend/govuk/all.scss"],
  "exports": {
    ".": {
      "sass": ["./nationalarchives/all.scss", "./node_modules/govuk-frontend/govuk/all.scss"],
